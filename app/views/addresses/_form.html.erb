<%= form_for :address, url: addresses_path do |f| %>

 <%= f.text_field :nickname, placeholder: 'Address Name' %>

 <%= f.text_field :address, placeholder: 'Address' %>

 <%= f.text_field :city , placeholder: 'City' %>

 <%= f.text_field :state , placeholder: 'State' %>

 <%= f.text_field :zip , placeholder: 'Zip Code' %>

   <!-- hidden field passing over user_id for strong params -->
   <%= f.hidden_field :user_id, :value => current_user.id %>

   <%= f.submit 'Save' , class: 'form-submit-btn' %>

<% end %>

