    <p><input name="roast" value= 'dark' type="checkbox" /> Dark</p>
</div>
<div>
  <h3>Region</h3>
    <p><input name="region" value ='latin_america' type="checkbox" /> Latin America</p>
    <p><input name="region" value = 'africa' type="checkbox" /> Africa</p>
